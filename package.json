{
  "name": "systemborn",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "build:netlify-function": "esbuild netlify/functions/react-router-server.js --bundle --platform=node --format=esm --outfile=netlify/functions/react-router-server.mjs --external:react-router --external:jsdom --external:react --external:react-dom --external:react-dom/server",
    "dev": "react-router dev",
    "start": "react-router-serve build/server/index.js",
    "typecheck": "tsc",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --fix --cache --cache-location ./node_modules/.cache/eslint ."
  },
  "dependencies": {
    "@fsoc/royalroadl-api": "^0.16.1",
    "@heroicons/react": "^2.0.18",
    "@react-router/dev": "^7.0.2",
    "@react-router/node": "^7.0.2",
    "@react-router/serve": "^7.0.2",
    "@tailwindcss/typography": "^0.5.10",
    "isbot": "^3.6.8",
    "jsdom": "^26.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.0.2"
  },
  "devDependencies": {
    "@netlify/vite-plugin-react-router": "^1.0.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.21",
    "esbuild": "^0.25.5",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6",
    "vite": "^5.0.12",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
